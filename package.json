{
  "name": "jin-themes",
  "displayName": "Jin Code Themes - Zen Collection",
  "description": "Thoughtfully crafted minimal light and dark theme for peaceful, distraction-free coding",
  "version": "1.0.3",
  "publisher": "Jijin",
  "license": "MIT",
"icon": "icons/icon.png",
  "galleryBanner": {
    "color": "#1F2430",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JijinJayakumar/jin-themes"
  },
  "homepage": "https://github.com/JijinJayakumar/jin-themes#readme",
  "badges": [
    {
      "url": "https://img.shields.io/visual-studio-marketplace/v/Jijin.jin-themes",
      "href": "https://marketplace.visualstudio.com/items?itemName=Jijin.jin-themes",
      "description": "VS Code Marketplace Version"
    }
  ],
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
      "commands": [
      {
        "command": "jinThemes.resetCursorSettings",
        "title": "Jin Themes: Reset Cursor Settings"
      }
    ],
    "themes": [
      {
        "label": "J charcoal",
        "uiTheme": "vs-dark",
        "path": "./themes/Jcharcoal-color-theme.json"
      },
      {
        "label": "J Dark Material v2",
        "uiTheme": "vs-dark",
        "path": "./themes/JDarkMaterial-color-theme.json"
      },
      {
        "label": "J Charcoal Light",
        "uiTheme": "vs",
        "path": "./themes/Jcharcoal-light-color-theme.json"
      },
      {
        "label": "J Funky Minimal Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Jfunky-minimal-dark-color-theme.json"
      }
    ],
    "configuration": {
      "title": "Jin Themes",
      "properties": {
        "jinThemes.cursorEffects": {
          "type": "object",
          "default": {
            "J charcoal": {
              "editor.cursorStyle": "line",
              "editor.cursorBlinking": "smooth",
              "editor.cursorSmoothCaretAnimation": "on",
              "editor.cursorWidth": 3,
              "editor.smoothScrolling": true
            },
            "J Dark Material v2": {
              "editor.cursorStyle": "line",
              "editor.cursorBlinking": "smooth",
              "editor.cursorSmoothCaretAnimation": "on",
              "editor.cursorWidth": 4,
              "editor.smoothScrolling": true
            },
            "J Charcoal Light": {
              "editor.cursorStyle": "line",
              "editor.cursorBlinking": "smooth",
              "editor.cursorSmoothCaretAnimation": "on",
              "editor.cursorWidth": 3,
              "editor.smoothScrolling": true
            },
            "J Funky Minimal Dark": {
              "editor.cursorStyle": "line",
              "editor.cursorBlinking": "smooth",
              "editor.cursorSmoothCaretAnimation": "on",
              "editor.cursorWidth": 4,
              "editor.smoothScrolling": true
            },
            "cursorSettings": {
              "J charcoal": {
                "background": "#000000",
                "foreground": "#FFCC66"
              },
              "J Dark Material v2": {
                "background": "#000000",
                "foreground": "#89DDFF"
              },
              "J Charcoal Light": {
                "background": "#FFFFFF",
                "foreground": "#FF9940"
              },
              "J Funky Minimal Dark": {
                "background": "#000000",
                "foreground": "#FF00FF"
              }
            },
            "cursorStyle": {
              "style": "line",
              "width": 2,
              "blinkAnimation": "smooth"
            }
          },
          "description": "Smooth thick cursor settings for different themes"
        }
      }
    }
  },
  "extensionDependencies": [],
  "recommendations": {
    "fonts": [
      {
        "name": "JetBrains Mono",
        "url": "https://www.jetbrains.com/lp/mono/"
      },
      {
        "name": "Fira Code",
        "url": "https://github.com/tonsky/FiraCode"
      }
    ]
  },
  "keywords": [
    "theme",
    "dark theme",
    "charcoal",
    "material",
    "jin themes",
    "jcharcoal",
    "J Dark Material v2",
    "Jin Code Themes",
    "Jijin",
    "material theme",
    "light theme",
    "charcoal light",
    "J Charcoal Light",
    "J Funky Minimal Dark",
    "funky minimal dark",
    "ji2n serenity",
    "serenity",
    "jin themes",
    "Jin Themes",
    "Ji2n Serenity", "Minimal Theme"
    
  ],
  "scripts": {
    "postinstall": "node ./scripts/configure-cursor.js",
    "preuninstall": "node ./scripts/cleanup-cursor.js",
    "configure": "node ./scripts/configure-cursor.js"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.94.0",
    "sharp": "^0.33.5"
  }
}
