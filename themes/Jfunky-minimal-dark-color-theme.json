{
  "name": "Funky Minimal Dark",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    // Base UI
    "editor.background": "#0D1117",
    "editor.foreground": "#E6E6E6",
    "activityBar.background": "#0D1117",
    "activityBar.foreground": "#FF00FF",
    "activityBar.inactiveForeground": "#4D4D4D",
    "activityBar.border": "#0D1117",
    "activityBar.activeBorder": "#FF00FF",
    "activityBarBadge.background": "#FF00FF",
    "activityBarBadge.foreground": "#0D1117",

    // Sidebar
    "sideBar.background": "#0D1117",
    "sideBar.foreground": "#B3B3B3",
    "sideBar.border": "#161B22",
    "sideBarTitle.foreground": "#FF00FF",
    "sideBarSectionHeader.background": "#161B22",
    "sideBarSectionHeader.foreground": "#E6E6E6",
    "sideBarSectionHeader.border": "#161B22",

    // Editor elements
    "editor.lineHighlightBackground": "#161B22",
    "editor.selectionBackground": "#264F78",
    "editor.findMatchBackground": "#FFD70033",
    "editor.findMatchHighlightBackground": "#FFD70019",
    "editorCursor.foreground": "#FF00FF",
    "editorLineNumber.foreground": "#4D4D4D",
    "editorLineNumber.activeForeground": "#FF00FF",
    "editorIndentGuide.background": "#1E2329",
    "editorIndentGuide.activeBackground": "#FF00FF40",

    // Buttons & inputs
    "button.background": "#FF00FF",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#FF33FF",
    "input.background": "#161B22",
    "input.border": "#30363D",
    "input.foreground": "#E6E6E6",
    "input.placeholderForeground": "#4D4D4D",

    // Tabs
    "tab.activeBackground": "#161B22",
    "tab.activeForeground": "#E6E6E6",
    "tab.activeBorder": "#FF00FF",
    "tab.inactiveBackground": "#0D1117",
    "tab.inactiveForeground": "#8B8B8B",
    "tab.border": "#161B22",
    "editorGroupHeader.tabsBackground": "#0D1117",
    "editorGroupHeader.tabsBorder": "#161B22",

    // Status bar
    "statusBar.background": "#161B22",
    "statusBar.foreground": "#8B8B8B",
    "statusBar.border": "#161B22",
    "statusBarItem.remoteBackground": "#FF00FF",
    "statusBarItem.remoteForeground": "#0D1117",

    // Terminal
    "terminal.background": "#0D1117",
    "terminal.foreground": "#E6E6E6",
    "terminal.ansiBlack": "#0D1117",
    "terminal.ansiBlue": "#00FFFF",
    "terminal.ansiBrightBlue": "#33FFFF",
    "terminal.ansiGreen": "#00FF00",
    "terminal.ansiBrightGreen": "#33FF33",
    "terminal.ansiMagenta": "#FF00FF",
    "terminal.ansiBrightMagenta": "#FF33FF",
    "terminal.ansiRed": "#FF0000",
    "terminal.ansiBrightRed": "#FF3333",
    "terminal.ansiYellow": "#FFD700",
    "terminal.ansiBrightYellow": "#FFDD33",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiBrightCyan": "#33FFFF",
    "terminal.ansiWhite": "#E6E6E6",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // Base declarations
    "*.declaration": "#FF00FF",

    // Classes and types
    "class": { "foreground": "#00FFFF", "fontStyle": "" },
    "class.declaration": { "foreground": "#00FFFF", "fontStyle": "bold" },
    "interface": { "foreground": "#00FFFF", "fontStyle": "italic" },
    "type": { "foreground": "#00FFFF", "fontStyle": "" },

    // Functions
    "function": { "foreground": "#FFD700", "fontStyle": "" },
    "function.declaration": { "foreground": "#FFD700", "fontStyle": "bold" },
    "method": { "foreground": "#FFD700", "fontStyle": "" },

    // Variables
    "variable": { "foreground": "#FF69B4", "fontStyle": "" },
    "variable.readonly": { "foreground": "#FF69B4", "fontStyle": "italic" },
    "variable.defaultLibrary": {
      "foreground": "#00FFFF",
      "fontStyle": "italic"
    },

    // Properties
    "property": { "foreground": "#E6E6E6", "fontStyle": "" },
    "property.readonly": { "foreground": "#E6E6E6", "fontStyle": "italic" },

    // Others
    "comment": { "foreground": "#4D4D4D", "fontStyle": "italic" },
    "string": { "foreground": "#00FF00", "fontStyle": "" },
    "number": { "foreground": "#FF00FF", "fontStyle": "" },
    "keyword": { "foreground": "#FF00FF", "fontStyle": "bold" }
  },
  "tokenColors": [
    // Comments
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#4D4D4D",
        "fontStyle": "italic"
      }
    },
    // Strings
    {
      "scope": ["string", "string.template"],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    // Numbers
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    // Keywords
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF00FF",
        "fontStyle": "bold"
      }
    },
    // Functions
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": ""
      }
    },
    // Classes
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#00FFFF",
        "fontStyle": "bold"
      }
    },
    // Variables
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    // Parameters
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FF69B4",
        "fontStyle": "italic"
      }
    },
    // Properties
    {
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    // Operators
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FF00FF",
        "fontStyle": "bold"
      }
    },
    // Punctuation
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    // Markdown
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#FF00FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#FF69B4",
        "fontStyle": "italic"
      }
    }
  ]
}
